<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net6.0</TargetFrameworks>
        <RootNamespace>OpenMedStack.Infrastructure.Tailf</RootNamespace>
        <AssemblyName>openmedstack.infrastructure.tailf</AssemblyName>
        <PackageId>OpenMedStack.Infrastructure.Tailf</PackageId>
        <PackageTags>OpenMedStack Architecture ASAP Tailf Infrastructure</PackageTags>
