    <Import Project="..\..\shared.proj" />

    <ItemGroup>
        <PackageReference Include="CommandLineParser" Version="2.8.0" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\openmedstack.infrastructure.bootstrapping\openmedstack.infrastructure.bootstrapping.csproj" />
    </ItemGroup>
</Project>